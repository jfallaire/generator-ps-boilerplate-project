<div class="coveo-result-frame">
  <div class="coveo-result-cell" style="vertical-align: top;">
    <div class="coveo-result-row" style="margin-top:0;">
      <div class="coveo-result-cell" style="vertical-align:top;font-size:14px;">
        <a class="CoveoSalesforceCommunityResultLink"></a>
      </div>
    </div>
    <div class="coveo-result-row">
      <div class="coveo-result-cell">
        <span class="CoveoExcerpt"></span>
      </div>
    </div>
    <div class="coveo-result-row">
      <div class="coveo-result-cell">
        <a class="CoveoSalesforceCommunityResultLink custom-printable-uri" data-use-as-printable="true"></a>
      </div>
    </div>
    <div class="coveo-result-row">
      <div class="coveo-result-cell">
        <span class="CoveoFieldValue custom-printable-date" data-field="@date" data-text-caption="Last Updated on" data-helper="date" data-helper-options="{useLongDateFormat: 'true'}"></span>
      </div>
    </div>
  </div>
</div>