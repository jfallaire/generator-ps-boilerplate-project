{
  "name": "generator-ps-boilerplate-project",
  "version": "1.0.0",
  "description": "Scaffolds out a template directory structure to help out CoveoPS folks when starting off a new project for a customer.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jfallaire/generator-ps-boilerplate-project.git"
  },
  "files": [
    "generators",
    "utils",
    "index.js"
  ],
  "main": "index.js",
  "keywords": [
    "yeoman-generator",
    "coveo",
    "search",
    "ui",
    "framework",
    "js",
    "sfdc",
    "bundle"
  ],
  "author": "jfallaire",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfallaire/generator-ps-boilerplate-project/issues"
  },
  "homepage": "https://github.com/jfallaire/generator-ps-boilerplate-project#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@semantic-release/changelog": "^3.0.1",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.5",
    "@semantic-release/npm": "^5.1.9",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "babel-eslint": "^7.1.1",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.2.0",
    "semantic-release": "^15.13.16",
    "semver": "^5.6.0",
    "standard-version": "^4.4.0",
    "travis-deploy-once": "^5.0.9"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "commit": "npx git-cz",
    "release": "standard-version -a",
    "postrelease": "git push --follow-tags",
    "test": "echo \"Error: no test specified\"",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "parse-author": "^2.0.0",
    "yeoman-generator": "^4.0.1",
    "yosay": "^2.0.2"
  }
}
